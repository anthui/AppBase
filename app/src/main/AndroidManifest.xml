<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ant.app_home">

    <application
        android:name="com.ant.app_base.AntApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        tools:replace="android:icon, android:label,android:theme">

        <activity android:name="com.ant.app_home.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <!--ã€€   &lt;!&ndash;<meta-data&ndash;&gt;-->
        <!--&lt;!&ndash;android:name="CHANNEL"&ndash;&gt;-->
        <!--&lt;!&ndash;android:value="${CHANNEL}" />&ndash;&gt;-->


    </application>

</manifest>